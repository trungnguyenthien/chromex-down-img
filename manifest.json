{
  "manifest_version": 3,
  "name": "Image Downloader",
  "version": "1.0",
  "permissions": [
    "downloads",
    "activeTab"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "web_accessible_resources": [
  {
    "resources": ["down.png"],
    "matches": ["<all_urls>"]
  }
]
}